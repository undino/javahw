<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>registration</servlet-name>
    <servlet-class>org.itstep.Servlets.ServletRegistration</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>registration</servlet-name>
    <url-pattern>/html/registration</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>into</servlet-name>
    <servlet-class>org.itstep.Servlets.ServletInto</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>into</servlet-name>
    <url-pattern>/html/into</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddMoneyServlet</servlet-name>
    <servlet-class>org.itstep.Servlets.AddMoneyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddMoneyServlet</servlet-name>
    <url-pattern>/html/addMoney</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddProductServlet</servlet-name>
    <servlet-class>org.itstep.Servlets.AddProductServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddProductServlet</servlet-name>
    <url-pattern>/html/addProduct</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>StoreServlet</servlet-name>
    <servlet-class>org.itstep.Servlets.StoreServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StoreServlet</servlet-name>
    <url-pattern>/html/store</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletTransaction</servlet-name>
    <servlet-class>org.itstep.Servlets.ServletTransaction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletTransaction</servlet-name>
    <url-pattern>/html/transaction</url-pattern>
  </servlet-mapping>
</web-app>
